    {
      "parameters": {
        "mode": "runOnceForEachItem",
        "jsCode": "// =======================\n// 1Ô∏è‚É£ DATE FORMATTER\n// =======================\nconst fmt = (d) => {\n  if (!d) return \"\";\n  const date = new Date(d);\n  if (isNaN(date)) return \"\";\n  return date.toLocaleString(\"en-US\", {\n    day: \"2-digit\",\n    month: \"2-digit\",\n    year: \"numeric\",\n    hour: \"2-digit\",\n    minute: \"2-digit\"\n  });\n};\n\n// =======================\n// 2Ô∏è‚É£ RDAP PARSE\n// =======================\nconst rdap = JSON.parse($json.data);\n\n// STATUS\nconst status = Array.isArray(rdap.status)\n  ? rdap.status.join(\", \").toLowerCase()\n  : (rdap.status || \"\").toLowerCase();\n\n// EVENTS\nconst events = Array.isArray(rdap.events) ? rdap.events : [];\nconst getEventDate = (type) =>\n  events.find(e => e.eventAction === type)?.eventDate || \"\";\n\n// Dates (RAW)\nconst expiry_raw = getEventDate(\"expiration\");\nconst created_raw = getEventDate(\"registration\");\nconst rdap_update_raw = getEventDate(\"last update of RDAP database\");\n\n// Formatted dates\nconst expiry_fmt = fmt(expiry_raw);\nconst created_fmt = fmt(created_raw);\nconst rdap_update_fmt = fmt(rdap_update_raw);\n\n// =======================\n// 4Ô∏è‚É£ NAMESERVERS\n// =======================\nconst nameservers = (rdap.nameservers || [])\n  .map(ns => ns.ldhName)\n  .filter(Boolean)\n  .join(\", \");\n\n// =======================\n// 5Ô∏è‚É£ SHEET‚ÄôTEN GELEN ESKƒ∞ VERƒ∞LER\n// =======================\n\n\nconst safe = (v) => (v === null || v === undefined) ? \"\" : String(v).trim();\n\nconst old_check  = safe($('Is Domain Supported?').item.json.old_check);\nconst old_status = safe($('Is Domain Supported?').item.json.old_status).toLowerCase();\nconst old_expiry = safe($('Is Domain Supported?').item.json.old_expiry);\n\n// üëâ FIRST CHECK RULE\nconst is_first_check = (old_status === \"\" && old_expiry === \"\");\n\n// =======================\n// 6Ô∏è‚É£ CHANGE CONTROLS\n// =======================\nconst changed_status = !is_first_check && status !== old_status;\nconst changed_expiry = !is_first_check && expiry_fmt !== old_expiry;\nconst has_change = is_first_check || changed_status || changed_expiry;\n\n// =======================\n// 7Ô∏è‚É£ EXPIRATION CALCULATIONS\n// =======================\nlet days_to_expiry = 0;\nlet is_expiring_soon = false;\n\nif (expiry_raw) {\n  const now = new Date();\n  const exp = new Date(expiry_raw);\n  days_to_expiry = Math.ceil((exp - now) / 86400000);\n  if (days_to_expiry < 0) days_to_expiry = 0;\n  is_expiring_soon = days_to_expiry <= 30;\n}\n\n// =======================\n// 8Ô∏è‚É£ DOMAIN\n// =======================\nconst domain = $('Prepare Data').item.json.domain;\n\n// =======================\n// 9Ô∏è‚É£ MESSAGE\n// =======================\nlet changeText = \"\";\n\nif (is_first_check) {\n  changeText = \"üÜï First check completed, sheet updated.\";\n} else {\n  const changes = [];\n  if (changed_status) {\n    changes.push(`üîÑ Status changed: ${old_status || \"-\"} ‚Üí ${status}`);\n  }\n  if (changed_expiry) {\n    changes.push(`üìÖ Expiration date changed: ${old_expiry || \"-\"} ‚Üí ${expiry_fmt}`);\n  }\n  changeText = changes.length ? changes.join(\"\\n\") : \"‚ÑπÔ∏è No changes.\";\n}\n\nconst message = `\nüåê Domain Check Automation\nüîó Domain: ${domain}\nüìå Current Status: ${status}\nüìÖ Expiration Date: ${expiry_fmt}\n‚è≥ Days Remaining: ${days_to_expiry}\n\n${changeText}\n\nüåç Nameservers:\n${nameservers || \"-\"}\n\nüïí Last Check: ${fmt(new Date().toISOString())}\n`.trim();\n\n// =======================\n// üîü RETURN (WITH DEBUG)\n// =======================\nreturn {\n  // üîç DEBUG\n  debug_old_status: old_status,\n  debug_old_expiry: old_expiry,\n  debug_old_check: old_check,\n  debug_new_status: status,\n  debug_new_expiry: expiry_fmt,\n\n  // ‚úÖ MAIN VALUES\n  new_status: status,\n  new_expiry: expiry_fmt,\n  created: created_fmt,\n  nameservers,\n  is_first_check,\n  has_change,\n  changed_status,\n  changed_expiry,\n  days_to_expiry,\n  is_expiring_soon,\n  rdap_last_update: rdap_update_fmt,\n  check_time: fmt(new Date().toISOString()),\n  message\n};"
      },

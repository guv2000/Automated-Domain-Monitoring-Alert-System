      "parameters": {
        "jsCode": "const raw = $input.first().json.Domain ?? \"\";\n\nconst domain = String(raw)\n  .trim()\n  .toLowerCase()\n  .replace(/^https?:\\/\\//, \"\")\n  .replace(/\\/.*$/, \"\");\n\n// 1Ô∏è‚É£ Is it technically a domain?\nconst isValidDomain =\n  domain.includes(\".\") &&\n  !domain.includes(\" \") &&\n  /^[a-z0-9.-]+$/.test(domain);\n\n// TLD\nconst tld = isValidDomain ? domain.split(\".\").pop() : null;\n\n// RDAP mapping\nconst rdapMap = {\n  com: \"https://rdap.verisign.com/com/v1/domain/\",\n  net: \"https://rdap.verisign.com/net/v1/domain/\",\n  name: \"https://rdap.verisign.com/name/v1/domain/\",\n\n  org: \"https://rdap.publicinterestregistry.org/rdap/domain/\",\n  info: \"https://rdap.afilias.net/rdap/domain/\",\n  biz: \"https://rdap.neustar.biz/rdap/domain/\",\n\n  guru: \"https://rdap.donuts.co/rdap/domain/\",\n  email: \"https://rdap.donuts.co/rdap/domain/\",\n  live: \"https://rdap.donuts.co/rdap/domain/\",\n  news: \"https://rdap.donuts.co/rdap/domain/\",\n  media: \"https://rdap.donuts.co/rdap/domain/\",\n  today: \"https://rdap.donuts.co/rdap/domain/\",\n  world: \"https://rdap.donuts.co/rdap/domain/\",\n  digital: \"https://rdap.donuts.co/rdap/domain/\",\n  expert: \"https://rdap.donuts.co/rdap/domain/\",\n  agency: \"https://rdap.donuts.co/rdap/domain/\",\n  technology: \"https://rdap.donuts.co/rdap/domain/\",\n  software: \"https://rdap.donuts.co/rdap/domain/\",\n  solutions: \"https://rdap.donuts.co/rdap/domain/\",\n  services: \"https://rdap.donuts.co/rdap/domain/\",\n  company: \"https://rdap.donuts.co/rdap/domain/\",\n\n  io: \"https://rdap.identitydigital.services/rdap/domain/\",\n  me: \"https://rdap.identitydigital.services/rdap/domain/\",\n  tv: \"https://rdap.identitydigital.services/rdap/domain/\",\n  cc: \"https://rdap.identitydigital.services/rdap/domain/\",\n  mobi: \"https://rdap.identitydigital.services/rdap/domain/\",\n  asia: \"https://rdap.identitydigital.services/rdap/domain/\",\n\n  dev: \"https://rdap.google/registry/domain/\",\n  app: \"https://rdap.google/registry/domain/\",\n  how: \"https://rdap.google/registry/domain/\",\n  page: \"https://rdap.google/registry/domain/\",\n  zip: \"https://rdap.google/registry/domain/\",\n  foo: \"https://rdap.google/registry/domain/\",\n  bar: \"https://rdap.google/registry/domain/\",\n\n  de: \"https://rdap.denic.de/domain/\",\n  fr: \"https://rdap.afnic.fr/domain/\",\n  nl: \"https://rdap.sidn.nl/domain/\",\n  eu: \"https://rdap.eu/domain/\",\n  uk: \"https://rdap.nominet.uk/domain/\",\n  it: \"https://rdap.nic.it/domain/\",\n  es: \"https://rdap.nic.es/domain/\",\n  ch: \"https://rdap.nic.ch/domain/\",\n  at: \"https://rdap.nic.at/domain/\",\n\n  se: \"https://rdap.iis.se/domain/\",\n  no: \"https://rdap.norid.no/domain/\",\n  dk: \"https://rdap.dk-hostmaster.dk/domain/\",\n  fi: \"https://rdap.traficom.fi/domain/\",\n\n  us: \"https://rdap.nic.us/domain/\",\n  ca: \"https://rdap.cira.ca/rdap/domain/\",\n  au: \"https://rdap.auda.org.au/domain/\",\n  jp: \"https://rdap.jprs.jp/domain/\",\n  cn: \"https://rdap.cnnic.cn/domain/\",\n  ru: \"https://rdap.tcinet.ru/domain/\",\n  br: \"https://rdap.registro.br/domain/\",\n};\n\n// 2Ô∏è‚É£ REAL supported definition\nconst supported = Boolean(isValidDomain && tld && rdapMap[tld]);\n\n$input.first().json.row_number\n\n\nreturn [{\n  domain,\n  tld,\n  supported,\n  rdapUrl: supported ? rdapMap[tld] + domain : \"\",\n  row_number: $input.first().json.row_number ?? null,\n  old_status: $json[\"Latest Status\"] || \"\",\n  old_expiry: $json[\"Expiration Date\"] || \"\",\n  old_check: $json[\"Last Check\"] || \"\"\n}];"
